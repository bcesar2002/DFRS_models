	<Function>
		<FuncName>the_tis_function</FuncName>
		<FuncTuple>
			<StaticGuard>"NOT(p_the_voltage <= 80) AND the_voltage <= 80"</StaticGuard>
			<TimedGuard>""</TimedGuard>
			<Statements>
				<VarName>the_indication_lights</VarName>
				<Expression>2</Expression>
